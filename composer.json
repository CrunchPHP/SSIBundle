{
    "name": "crunch/ssi-bundle",
    "description": "Server Side Includes (SSI) fragment renderer for Symfony2 applications",
    "type": "symfony-bundle",
    "keywords": ["ssi"],
    "homepage": "https://github.com/KingCrunch/SSIBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Krebs",
            "email": "sebastian@krebs.one",
            "homepage": "https://www.kingcrunch.eu",
            "role": "Maintainer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "symfony/http-foundation": "~2.3|~3.0",
        "symfony/config": "~2.3|~3.0",
        "symfony/dependency-injection": "~2.3|~3.0",
        "symfony/http-kernel": "~2.3|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "support": {
        "issues": "https://github.com/KingCrunch/SSIBundle/issues"
    },
    "autoload": {
        "psr-0": {
            "Crunch\\Bundle\\SSIBundle": ""
        }
    },
    "target-dir": "Crunch/Bundle/SSIBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    }
}
