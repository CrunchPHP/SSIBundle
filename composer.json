{
    "name": "crunch/ssi-bundle",
    "description": "Server Side Includes (SSI) fragment renderer for Symfony2 applications",
    "type": "symfony-bundle",
    "keywords": ["ssi"],
    "homepage": "https://github.com/CrunchPHP/SSIBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Krebs",
            "email": "krebs.seb@gmail.com",
            "homepage": "http://www.kingcrunch.de",
            "role": "Maintainer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "symfony/http-foundation": ">=2.2,<2.4-dev",
        "symfony/config": ">=2.2,<2.4-dev",
        "symfony/dependency-injection": ">=2.2,<2.4-dev",
        "symfony/http-kernel": ">=2.2,<2.4-dev"
    },
    "support": {
        "email": "krebs.seb@gmail.com",
        "issues": "https://github.com/CrunchPHP/SSIBundle/issues"
    },
    "autoload": {
        "psr-0": {
            "Crunch\\Bundle\\SSIBundle": ""
        }
    },
    "target-dir": "Crunch/Bundle/SSIBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
